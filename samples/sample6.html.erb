<h1>Listing otml_files</h1>

<table>
  <tr>
    <th>Uuid</th>
    <th>Otml category</th>
    <th>Name</th>
    <th>Path</th>
    <th>Content</th>
  </tr>

<% @otml_files.each do |otml_file| %>
  <tr>
    <td><%=h otml_file.uuid %></td>
    <td><%=h otml_file.otml_category_id %></td>
    <td><%=h otml_file.name %></td>
    <td><%=h otml_file.path %></td>
    <td><%=h otml_file.content %></td>
    <td><%= link_to 'Show', otml_file %></td>
    <td><%= link_to 'Edit', edit_otml_file_path(otml_file) %></td>
    <td><%= link_to 'Destroy', otml_file, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New otml_file', new_otml_file_path %>